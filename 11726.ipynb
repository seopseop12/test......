   "source": [
    "n = int(input())\n",
    "\n",
    "dp = [0 for _ in range(n+1)]\n",
    "\n",
    "if n <= 3 : print(n)\n",
    "else : \n",
    "\tdp[1] = 1\n",
    "\tdp[2] = 2\n",
    "\tfor i in range(3, n+1):\n",
    "\t\tdp[i] = dp[i-1] + dp[i-2]\n",
    "\n",
    "\tprint(dp[i]%10007)"
   ]
  }
